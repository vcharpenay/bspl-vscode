{
	"$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
	"name": "BSPL",
	"patterns": [
		{
			"name": "keyword.control",
			"match": "\\b(protocol|roles|parameters|private)\\b"
		},
		{
			"name": "comment.line.double-slash",
			"begin": "//",
			"end": "$"
		},
		{
			"name": "keyword",
			"match": "\\b(out|in|nil|any|opt)\\b"
		},
		{
			"name": "keyword.operator",
			"match": "\\b(key)\\b"
		},
		{
			"name": "keyword.operator",
			"match": "\\b(->|→|↦)\\b"
		}
	],
	"repository": {},
	"scopeName": "source.bspl"
}