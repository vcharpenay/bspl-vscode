{
  "name": "bspl-vscode",
  "displayName": "BSPL",
  "description": "BSPL language features in VS Code",
  "version": "0.1.0",
  "publisher": "vcharpenay",
  "icon": "images/icon.png",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "repository": "https://github.com/vcharpenay/bspl-vscode",
  "main": "./src/bspl.js",
  "contributes": {
    "languages": [{
      "id": "bspl",
      "aliases": ["BSPL", "bspl"],
      "extensions": [".bspl"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "bspl",
      "scopeName": "source.bspl",
      "path": "./syntaxes/bspl.tmLanguage.json"
    }]
  }
}
